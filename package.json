{
  "name": "@qikify/polaris-vue",
  "version": "0.1.6",
  "description": "Shopify Polaris UI library for Vue 2",
  "author": {
    "name": "Qikify"
  },
  "keywords": [
    "polaris",
    "shopify polaris",
    "polaris-vue"
  ],
  "main": "dist/polaris-vue.js",
  "module": "dist/polaris-vue.js",
  "files": [
    "dist",
    "package.json",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "serve": "yarn run copy-polaris-tokens && vue-cli-service serve",
    "build": "rimraf dist && yarn run copy-polaris-tokens && cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "lint": "vue-cli-service lint",
    "storybook": "yarn run copy-polaris-tokens && start-storybook -p 6006",
    "build-storybook": "yarn run copy-polaris-tokens && rimraf ./docs && build-storybook -o docs",
    "copy-polaris-tokens": "rimraf ./node_modules/polaris-react/src/styles/polaris-tokens && shx cp -r ./node_modules/@shopify/polaris-tokens/dist ./node_modules/polaris-react/src/styles/polaris-tokens"
  },
  "dependencies": {
    "@shopify/polaris-icons": "^4.11.0",
    "@shopify/polaris-tokens": "^4.0.0",
    "core-js": "^3.6.5",
    "lodash": "^4.17.21",
    "vue": "^2.6.14",
    "vue-svg-loader": "^0.16.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@rollup/plugin-alias": "^3.1.8",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@storybook/addon-docs": "^6.4.8",
    "@storybook/addon-essentials": "^6.4.8",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/addons": "^6.4.8",
    "@storybook/preset-typescript": "^3.0.0",
    "@storybook/theming": "^6.4.8",
    "@storybook/vue": "^6.4.8",
    "@types/lodash": "^4.14.178",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "^8.2.3",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5",
    "eslint": "7.14.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-typescript": "^0.14.0",
    "eslint-plugin-vue": "^8.1.0",
    "minimist": "^1.2.5",
    "path": "^0.12.7",
    "polaris-react": "https://github.com/Shopify/polaris-react.git",
    "portal-vue": "^2.1.7",
    "postcss": "^7.0.18",
    "postcss-modules": "^3.1.0",
    "pug-plain-loader": "^1.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.61.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.1",
    "rollup-plugin-vue": "^5.1.9",
    "rollup-plugin-vue-inline-svg": "^1.1.2",
    "sass": "~1.32.13",
    "sass-loader": "^10",
    "shx": "^0.3.3",
    "style-loader": "^2",
    "ts-dedent": "^2.2.0",
    "ttypescript": "^1.5.13",
    "typescript": "~4.1.5",
    "typescript-plugin-css-modules": "^3.4.0",
    "vue-class-component": "^7.2.3",
    "vue-css-modules": "^1.2.0",
    "vue-loader": "^15.9.8",
    "vue-property-decorator": "^9.1.2",
    "vue-router": "^3.5.3",
    "vue-template-compiler": "^2.6.11"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/qikify/polaris-vue.git"
  },
  "bugs": {
    "url": "https://github.com/qikify/polaris-vue/issues"
  },
  "homepage": "https://github.com/qikify/polaris-vue#readme",
  "license": "MIT"
}
